# Configuration Discord Bot
DISCORD_TOKEN=your_discord_bot_token_here

# Configuration MongoDB Atlas (requis pour le système de bump)
MONGODB_USERNAME=your_mongodb_username
MONGODB_PASSWORD=your_mongodb_password
MONGODB_CLUSTER_URL=cluster0.xxxxx.mongodb.net

# Configuration optionnelle
NODE_ENV=development

# Configuration yt-dlp pour le système musique
YTDLP_SOCKET_TIMEOUT=10
YTDLP_NO_CHECK_CERT=false

# Configuration des APIs bump (optionnel - pour les vraies intégrations)
TOPGG_TOKEN=your_topgg_token
DISCORDBOTLIST_TOKEN=your_discordbotlist_token
DISBOARD_TOKEN=your_disboard_token

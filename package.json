{
  "name": "bag-bot-v2-test",
  "version": "2.0.0",
  "description": "BAG Bot V2 - Test Dashboard",
  "main": "index.js",
  "scripts": {
    "start": "node index.render-final.js",
    "deploy:commands": "node force-register.js",
    "deploy:guild": "node scripts/register-guild.js",
    "reset:commands": "node scripts/remove-commands.js && node force-register.js",
    "remove:commands": "node scripts/remove-commands.js",
    "diagnose:music": "node -e \"console.log('LOAD'); require('./managers/SimpleMusicManager'); console.log('OK')\"",
    "diag:yt-dlp": "node node_modules/@distube/yt-dlp/bin/yt-dlp --version",
    "diag:voice": "node -e \"try{console.log(require('@discordjs/voice').generateDependencyReport())}catch(e){console.log('no-report', e?.message)}\"",
    "diag:lavalink": "node -e \"try{const M=require('./managers/LavalinkMusicManager');const p=M.__getConfiguredNodesPreview?.()||{nodes:[],source:'unknown'};console.log('configured',p.source,p.nodes.length);console.log('has lib',!!require('shoukaku'));console.log('first',p.nodes[0]);}catch(e){console.log('diag error',e?.message)}\"",
    "test": "echo \"Test passed\" && exit 0",
    "postinstall": "node scripts/install-yt-dlp.js"
  },
  "dependencies": {
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "is-ci": "^3.0.1",
    "libsodium-wrappers": "^0.7.13",
    "mongodb": "^6.18.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.34.3",
    "shoukaku": "^4.1.1"
  },
  "engines": {
    "node": "20.x"
  }
}
